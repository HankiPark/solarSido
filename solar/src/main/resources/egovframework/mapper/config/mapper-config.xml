<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<!--Mybatis 설정 -->
	<settings>
		<!-- 전통적인 데이터베이스 컬럼명 형태인 A_COLUMN을 CamelCase형태의 자바 프로퍼티명 형태인 aColumn으로 자동으로 매핑하도록 함 -->
		<setting name="mapUnderscoreToCamelCase" value="true"></setting>
		<!--  파라미터에 Null 값이 있을 경우 에러 처리 -->
		<setting name="jdbcTypeForNull" value="VARCHAR"></setting>
	</settings>
	
	<!-- Type Aliases 설정-->
	<typeAliases>
		<typeAlias alias="egovMap" 			type="egovframework.rte.psl.dataaccess.util.EgovMap" />
		<typeAlias alias="FileVO"			type="egovframework.com.cmm.service.FileVO" />
		<typeAlias alias="ComDefaultCodeVO" type="egovframework.com.cmm.ComDefaultCodeVO" />
		<typeAlias alias="comDefaultVO"		type="egovframework.com.cmm.ComDefaultVO" />
		<typeAlias alias="Order"		type="solar.sales.order.dao.Order" />
		<typeAlias alias="Prdt"		type="solar.sales.inout.dao.Prdt" />
		<typeAlias alias="EqmVO"		type="solar.eqm.mng.service.EqmVO" />
		<typeAlias alias="ProdPlanVO"		type="solar.prod.plan.service.ProdPlanVO" />
		<typeAlias alias="IndicaVO"		type="solar.prod.indica.service.IndicaVO" />
		<typeAlias alias="Menu"		type="solar.menu.dao.Menu" />
		<typeAlias alias="PrdtChase"		type="solar.prdt.dao.PrdtChase" />
		<typeAlias alias="MessageVO"		type="solar.wsk.msg.dao.MessageVO" />
		<typeAlias alias="Prcs2"		type="solar.prcs2.dao.Prcs2" />
		<typeAlias alias="Calendar"		type="solar.sales.order.dao.CalendarVO" />
	</typeAliases>
    
</configuration>